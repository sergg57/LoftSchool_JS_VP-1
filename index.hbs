<div class ='wrap'>
    <div  id="result"> </div>
</div>
<script>
    const template = '<div><b>{{name}} {{lastname}}</b></div>';
    const render = Handlebars.compile(template);
    const html1 = render({name: 'Иван', lastname: 'Петров'});
    const html2 = render({name: 'Пётр', lastname: 'Иванов'});

    const result = document.querySelector('#result');
    result.innerHTML = html1;
    result.innerHTML += html2;
    //console.log(html1, html2);
</script>

